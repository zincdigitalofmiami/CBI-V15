# ============================================================================
# CBI-V15 Environment Variables Template
# ============================================================================
# Copy this file to .env and fill in your actual API keys
# DO NOT commit .env to git (it's in .gitignore)
# ============================================================================

# ============================================================================
# Database (CRITICAL - Required)
# ============================================================================
MOTHERDUCK_TOKEN=your_motherduck_token_here
MOTHERDUCK_DB=cbi-v15

# ============================================================================
# Market Data (CRITICAL - Required)
# ============================================================================
DATABENTO_API_KEY=your_databento_api_key_here

# ============================================================================
# Economic Data (IMPORTANT - Recommended)
# ============================================================================
FRED_API_KEY=your_fred_api_key_here

# ============================================================================
# Energy Data (IMPORTANT - Recommended)
# ============================================================================
EIA_API_KEY=your_eia_api_key_here

# ============================================================================
# Weather Data (IMPORTANT - Recommended)
# ============================================================================
NOAA_API_TOKEN=your_noaa_token_here

# ============================================================================
# Sentiment Data (IMPORTANT - Recommended)
# ============================================================================
SCRAPECREATORS_API_KEY=your_scrapecreators_key_here

# ============================================================================
# Agricultural Data (OPTIONAL)
# ============================================================================
USDA_NASS_API_KEY=your_usda_nass_key_here

# ============================================================================
# Alternative Market Data (OPTIONAL)
# ============================================================================
POLYGON_API_KEY=your_polygon_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# ============================================================================
# Advanced Weather (OPTIONAL)
# ============================================================================
COPERNICUS_API_KEY=your_copernicus_key_here

# ============================================================================
# Shipping Data (OPTIONAL)
# ============================================================================
MARINETRAFFIC_API_KEY=your_marinetraffic_key_here

# ============================================================================
# Trading Economics (OPTIONAL)
# ============================================================================
TRADINGECONOMICS_CLIENT=your_client_id_here
TRADINGECONOMICS_API_KEY=your_tradingeconomics_key_here

# ============================================================================
# Application Settings
# ============================================================================
ENVIRONMENT=development  # development, staging, production
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR

