-- Validation Set View
-- Filters daily_ml_matrix to validation date range

config {
  type: "view",
  schema: "training",
  name: "daily_ml_matrix_val",
  description: "Validation set: 2019-01-01 to 2021-12-31"
}

SELECT *
FROM ${ref("daily_ml_matrix")}
WHERE date BETWEEN DATE '2019-01-01' AND DATE '2021-12-31'

